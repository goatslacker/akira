camelize(str) -> str.replace: /(-|_)+([a-zA-Z0-9$_])/g, (\i -> i.2.to-upper-case: ())
uppercase(str) -> str.to-upper-case: ()
capitalize(str) -> at: 1, str |> uppercase: @ |> concat: @, (str.slice: 1)
chars(fn, str) -> str.split: '' |> map: fn, @
compact(str) -> str.replace: ' ', ''
lines(fn, str) -> str.split: '\n' |> map: fn, @
pad(padding, str) -> construct Array: padding |> join: @ |> concat: @, str |> concat: str, @
repeat(times, str) -> construct Array: times |> join: str, @
words(fn, str) -> str.split: ' ' |> map: fn, @
