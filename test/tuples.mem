# test function
tupleEq(a, b) -> eq: 0, (length: (filter: (\x, i -> neq: x, (at: b, i)), a))

# setting tuples
obj = { hello = 1, world = 2 }
tupleEq: obj, { hello = 1, world = 2 }

# retrieving properties
eq: (get: obj, 'hello'), 1 | assert

# calling functions
myFunction(x) -> x
tup = {
  myFunction = myFunction
  myNumber = 3
}

eq: (call: tup, 'myFunction', 2), 2 | assert
assert: tup.myNumber == 3

