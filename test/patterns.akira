numbers = fn
  ['one'] 1
  ['two'] 2
  ['three'] 3
  [four] 4

ok: 4 (numbers: 'four')
ok: 1 (numbers: 'one')

gt-ten = fn
  [(< 10)] true
  [(> 10)] false

ok: false (gt-ten: 11)
ok: true (gt-ten: 2)

starts-with = fn [noun, prefix] prefix == noun.1 + noun.2

spanish-gender-of-noun = fn
  [(starts-with: &0, 'el')] 'male'
  [(starts-with: &0, 'la')] 'female'

ok: 'female', (spanish-gender-of-noun: 'la casa')
ok: 'male', (spanish-gender-of-noun: 'el carro')

sort-even-odd = fn
  [[]] []
  [[x & xs]] {
    if (odd: x)
      then (sort-even-odd: xs) ++ [x]
      else x +: (sort-even-odd: xs)
  }

[1 2 3 4 5 6] | sort-even-odd | ok-deep: [2 4 6 5 3 1]

-- implementations of our util functions
init = fn [[& a b]] a
tail = fn [[a & b]] b
head = fn [[a & b]] a
last = fn [[& a b]] b
get-middle = fn [[a & b c]] b


ls = [1 2 3 4 5]

ls | get-middle | ok-deep: [2 3 4]
ls | init | ok-deep: [1 2 3 4]
ls | tail | ok-deep: [2 3 4 5]
ls | head | ok: 1
ls | last | ok: 5

add-two = fn
  [0] 0
  [n] n + 2

add-two: 0 | ok: 0
add-two: 2 | ok: 4
