[a b] = [1 2]
assert a 1
assert b 2
[c d] = if true then if false then [0 1] else [2 3] else [4 5]
assert c 2
assert d 3
[e] = [0]
assert e 0

{@f @g} = { @f 1 @g 4 }
assert f 1
assert g 4
{@h} = if false then { @h 1 } else { @h 2 }
assert h 2

i = 3
{@j} = {@j i}
assert j 3

{@k l} = {@l 6}
assert k 6

[m n [o]] = [1 2 [3]]
assert m 1
assert n 2
assert o 3

[{ @p }] = [{ @p 1 }]
assert p 1

{ @q @r { @s } } = { @q 1 @r { @s 2 }}
assert s 2

{ @t u } = { @u 1 }
assert t 1

[_ _ _ v] = [5 6 7 8]
assert v 8

[_ [_ w x]] = ['a' ['b' 'c' 'd']]
assert w 'c'
assert x 'd'
