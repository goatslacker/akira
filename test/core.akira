curried-ok = curry ok
is-false = curried-ok false
is-true = curried-ok true

-- curry
curried-flip = curry flip
silly-id = curried-flip id 1
silly-id 2 | ok 2

curried-sum = curry sum
add-one = curried-sum 1
2 | add-one | ok 3


obj = { @foo 1 @bar 2 @baz 3 }

-- keys
obj | keys | ok-deep ['foo' 'bar' 'baz']

-- values
obj | values | ok-deep [1 2 3]

-- key
obj | key 2 | ok 'bar'

-- value
obj | value 'foo' | ok 1

-- inc
inc 1 | ok 2

-- dec
dec 2 | ok 1

-- false?
false? false | is-true
false? true | is-false

-- true?
true? false | is-false
true? true | is-true

-- pos?
pos? 3 | is-true
pos? -1 | is-false
pos? 0 | is-false

-- neg?
neg? 3 | is-false
neg? -1 | is-true
neg? 0 | is-false

-- zero?
zero? 0 | is-true

-- vector?
obj | vector? | is-false
[1 2 3] | vector? | is-true
2 | vector? | is-false

-- map?
obj | map? | is-true
[1 2 3] | map? | is-false

-- str?
'hello' | str? | is-true
2 | str? | is-false

-- pow
map (** 2) [1 2 3] | ok-deep [1 4 9]

-- sum
ok 2 (sum 1 1)

-- prod
ok 4 (prod 2 2)
