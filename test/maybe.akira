safe-head = fn [v] do {
  v !! 0
}

safe-head [1] | ok 1
safe-head none | ok none

a = { @foo 'bar' }
b = { @baz 'bar' }

get-bar = fn [obj] maybe obj.foo { obj.foo }

get-bar a | ok 'bar'
get-bar b | ok none

add-to-four = fn [a]
  let b = 4
  then maybe a {
    a + b
  }

add-to-four none | ok none
add-to-four 3 | ok 7
