-- I have to make a Animal, Cat example...
Animal = {
  @speak (fn [] 'my name is ' ++ this.name)
  @walk (fn [direction] true)
  @sleep (fn [] 'ZzZzZ...')
  @alive true
  @eyes 2
}

Cat = {
  @speak (fn [] 'meow i am ' ++ this.name)
  @sleep (fn [] 'Purr ' ++ (prototype.sleep!))
  @legs 4
} Animal

cat = { @name 'Luna' } Cat
wasabi = { @name 'Wasabi' } Cat

cat.speak! | assert: 'meow i am Luna'
cat.alive | assert: true
cat.eyes | assert: 2
cat.legs | assert: 4
cat.walk! | assert: true
cat.sleep! | assert: 'Purr ZzZzZ...'

wasabi.speak! | assert: 'meow i am Wasabi'
