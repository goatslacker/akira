Animal = {
  @speak (fn [] 'my name is ' ++ this.name)
  @walk (fn [direction] true)
  @sleep (fn [] 'ZzZzZ...')
  @alive true
  @eyes 2
}

Cat = Animal {
  @speak (fn [] 'meow i am ' ++ this.name)
  @sleep (fn [] 'Purr ' ++ (prototype.sleep!))
  @legs 4
}

cat = Cat { @name 'Luna' }
wasabi = Cat { @name 'Wasabi' }

cat.speak! | ok: 'meow i am Luna'
cat.alive | ok: true
cat.eyes | ok: 2
cat.legs | ok: 4
cat.walk! | ok: true
cat.sleep! | ok: 'Purr ZzZzZ...'

wasabi.speak! | ok: 'meow i am Wasabi'

