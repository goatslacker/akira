-- test function
map-eq :: Map Map -> Boolean
map-eq = fn [a b]
  filter (fn [x i]
    x isnt b !! i
  ) a | (.length) | assert 0

-- setting maps
obj = { @hello 1 @world 2 }
map-eq obj { @hello 1 @world 2 }

-- retrieving properties
assert (get obj 'hello') 1

-- calling functions
tup = {
  @my-function id
  @my-number 3
}

assert (call tup 'myFunction' 2) 2
assert tup.my-number 3
assert (tup.myFunction 2) 2
assert (tup.my-function 'a') 'a'
