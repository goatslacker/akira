a = [1, 2, 3]

square-list = foldl => flip => square
a | square-list | assert: 9

assert-deep: [1] ++ [2], [1, 2]

assert-deep: 1 +: [2], [1, 2]

assert-deep: [1] ++ [2] ++ [3], [1, 2, 3]

assert: not true, false
assert: not false, true

assert: 1 < 2, true
assert: 2 > 1, true
assert: 2 >= 2, true
assert: 3 <= 1, false

assert: 1 + 1, 2
assert: 1 - 1, 0
assert: 3 * 0, 0
assert: 4 / 2, 2

a | foldl: + | assert: 6
a | foldl: * | assert: 6
a | map: (* 2) | assert-deep: [2, 4, 6]
a | map: (- 1) | assert-deep: [0, 1, 2]
