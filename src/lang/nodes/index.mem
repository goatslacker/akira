path = require: 'path'

r = fn [m] m.to-lower-case! | path.join: __dirname | require

export {
  @Access         (r: 'access')
  @Arguments      (r: 'arguments')
  @Assignment     (r: 'assignment')
  @Call           (r: 'call')
  @Compare        (r: 'compare')
  @Comprehension  (r: 'comprehension')
  @Construction   (r: 'construction')
  @Declaration    (r: 'declaration')
  @Exception      (r: 'exception')
  @Export         (r: 'export')
  @If             (r: 'if')
  @Identifier     (r: 'identifier')
  @Import         (r: 'import')
  @L              (r: 'line')
  @Literal        (r: 'literal')
  @List           (r: 'list')
  @Macro          (r: 'macro')
  @Map            (r: 'map')
  @Nodes          (r: 'nodes')
  @Objects        (r: 'objects')
  @Operation      (r: 'operation')
  @Pattern        (r: 'pattern')
  @Pipeline       (r: 'pipeline')
  @Recur          (r: 'recur')
}
